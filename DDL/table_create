CREATE TABLE Unidades(ID INT PRIMARY KEY, 
                  NAME VARCHAR(255),
				  abcissa DOUBLE,
				  ordenada DOUBLE,
        		  video BOOLEAN,
				  termometro BOOLEAN,
			      co2 BOOLEAN,
			  	  ch4 BOOLEAN,
			  	  status BOOLEAN);
			  	  
			  	  
